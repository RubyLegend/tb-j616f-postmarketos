+static irqreturn_t mt6375_pd_evt_handler(int irq, void *data)
+{
+	struct mt6375_tcpc_data *ddata = data;
+
+	MT6375_DBGINFO("++\n");
+	pm_stay_awake(ddata->dev);
+	disable_irq_nosync(ddata->irq);
+	kthread_queue_work(&ddata->irq_worker, &ddata->irq_work);
+	return IRQ_HANDLED;
+}
