+static int __init rt5081_init(void)
+{
+	struct device_node *np;
+
+	pr_info("rt5081h_init (%s): initializing...\n", RT5081_DRV_VERSION);
+	np = of_find_node_by_name(NULL, "usb_type_c");
+	if (np != NULL)
+		pr_info("usb_type_c node found...\n");
+	else
